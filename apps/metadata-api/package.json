{
  "name": "@apps/metadata-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "dotenv -e ../../.env -- tsx watch src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "temporal:worker": "dotenv -e ../../.env -- tsx src/temporal/worker.ts",
    "prisma": "prisma",
    "test:signals": "tsx src/signals/signalStore.spec.ts && tsx src/signals/signalEvaluator.spec.ts",
    "test:brain": "node --import tsx --test src/brain/indexProfiles.test.ts src/brain/indexerWorkDoc.test.ts src/brain/vectorSearch.test.ts src/brain/indexMetadataNormalization.test.ts src/brain/clustersRegistry.test.ts src/brain/clustersBuild.test.ts src/brain/clustersIdempotency.test.ts src/brain/clustersRead.test.ts src/brain/brainSearch.test.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@grpc/grpc-js": "^1.14.2",
    "@grpc/proto-loader": "^0.8.0",
    "@metadata/client": "workspace:*",
    "@metadata/core": "workspace:*",
    "@prisma/client": "^5.22.0",
    "@temporalio/client": "^1.13.1",
    "@temporalio/worker": "^1.13.1",
    "@temporalio/workflow": "^1.13.1",
    "execa": "^8.0.1",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.25.0",
    "jose": "^5.2.4",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/pg": "^8.11.6",
    "prisma": "^5.22.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5"
  }
}
