          }
        ]
      }
    },
    {
      "id": "sample_catalog_confluence_pages",
      "labels": ["sample", "confluence", "docs"],
      "sourceEndpointId": "seed-endpoint-confluence",
      "payload": {
        "dataset": {
          "id": "sample_catalog_confluence_pages",
          "displayName": "Seeded Confluence Pages",
          "description": "Synthetic documentation pages for smoke testing doc catalog flows.",
        "fields": [
          { "name": "page_id", "type": "STRING", "description": "Confluence page id" },
          { "name": "space_key", "type": "STRING", "description": "Owning space key" },
          { "name": "title", "type": "STRING", "description": "Page title" },
          { "name": "updated_at", "type": "TIMESTAMP", "description": "Last updated timestamp" },
          { "name": "updated_by", "type": "STRING", "description": "Display name of the last editor" },
          { "name": "url", "type": "STRING", "description": "Confluence page URL" },
          { "name": "excerpt", "type": "STRING", "description": "Rendered body preview" }
        ]
      },
      "schema": "CONFLUENCE",
      "name": "confluence_pages",
      "description": "Sample knowledge pages used to exercise metadata previews.",
        "labels": ["confluence", "documentation", "sample"],
        "statistics": {
          "record_count": 25,
          "sample_size": 5,
          "last_profiled_at": "2024-10-05T00:00:00Z"
        },
